        "plt.scatter(months, city_B_temps, c='red', marker='*', label='City B')\n",
        "plt.xlabel('Months')\n",
        "plt.ylabel('Average Temperature (°F)')\n",
        "plt.title('Average Temperature Comparison')\n",
        "plt.legend()\n",
        "plt.show()"
