      "source": [
        "# x axis values\n",
        "x = [1,2,3,4,5]\n",
        "# corresponding y axis values\n",
        "y1 = [2,4,6,8,10]\n",
        "y2 = [0.5,1,1.5,2,2.5]\n",
        "y3 = [1,4,9,16,25]\n",
        "\n",
        "bar_width = 0.6\n",
        "bar_positions = np.arange(len(x))\n",
        "fig, ax = plt.subplots()\n",
        "ax.bar(bar_positions, y1, width=bar_width, label='y1', color='blue')\n",
        "ax.bar(bar_positions, y2, width=bar_width, label='y2', color='green', alpha=0.7)\n",
        "ax.bar(bar_positions, y3, width=bar_width, label='y3', color='red', alpha=0.5)\n",
        "ax.set_xlabel('Favorite Color', fontsize=12)\n",
        "ax.set_ylabel('Number of People', fontsize=12)\n",
        "ax.set_title('Number of People vs Favorite Color', fontsize=20)\n",
        "ax.set_xticks(bar_positions)\n",
        "ax.set_xticklabels(x)\n",
        "ax.legend()\n",
        "plt.show()"
